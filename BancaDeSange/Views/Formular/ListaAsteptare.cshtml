@model IEnumerable<BancaDeSange.Models.Formular>

@{
    ViewBag.Title = "ListaAsteptare";
}

<h2>Lista de Asteptare</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Institutia)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Grupa)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Cantitatea)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Rh_ul)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Institutia)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Grupa)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Cantitatea)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Rh_ul)
        </td>
        
    </tr>
}

</table>
